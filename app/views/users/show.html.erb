<div class="contents row">
  <p><%= @nickname %>さんの投稿一覧</p>
  <% @musicians.each do |musician| %>
    <div class="content_post" style="background-image: url(<%= musician.image %>);">
      <p><%= musician.musician_name %></p>
      <span class="name"><%= musician.musician_name %></span>
    </div>
  <% end %>
</div>