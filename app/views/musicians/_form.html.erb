<%= form_with(model: musician, local: true) do |form| %>
      <%= form.text_field :musician_name, placeholder: "Nickname" %>
      <%= form.file_field  :image, placeholder: "画像" %>
      <%= form.text_field :date_of_birth, placeholder: "Nickname" %>
      <%= form.text_area :musician_view, placeholder: "text", rows: "10" %>
      <%= form.text_area :musician_song, placeholder: "text", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>